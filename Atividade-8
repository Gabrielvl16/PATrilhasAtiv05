using System;

namespace b
{
	class Program
	{
		public static void Main()
		{
			// Recebendo texto do úsuario
			string texto = Console.ReadLine(); 
			
			// Invertendo valores
            int numero = int.TryParse(texto, out int resultado) ? resultado : 0;

            // Exibindo resultado
            Console.WriteLine("O número é " +numero);
 
            Console.Write("Digite qualquer tecla para sair . . . ");
            Console.ReadKey(true);
		}
	}
}
